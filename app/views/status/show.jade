body
  h2 Eva Presentation Status

  .endpoints
    h4 Endpoints
    table
      each value, key in keys
        tr.table.table-bordered.table-striped.table-hover.table-condensed
          td
            strong= key
          td= value.myInnerKey

  label Name:
  input(type="text", ng-model="welcomeText").welcomeField
  hr
  .welcomeText Welcome to {{welcomeText}}
